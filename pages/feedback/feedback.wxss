/* pages/feedback/feedback.wxss */
page{
  background-color: #f5f5f5;
}
.p_tabs_title{
  background-color: #fff;
}
.fb_main{
  padding: 20rpx;
  font-size: 28rpx;
}
.fb_tips{
  display: flex;
  flex-wrap: wrap;
}
.fb_tips text{
  width: 30%;
  padding: 10rpx;
  margin:20rpx 10rpx;
  background-color: #fff;
  text-align: center;
}
.fb_content{
  background-color: #fff;
}
.fb_content textarea{
  padding: 10rpx;
}
.fb_tool{
  display: flex;
  flex-wrap: wrap;
  padding-top: 20rpx;
}
.fb_tool button{
  width: 120rpx;
  height: 120rpx;
  padding: 0;
  font-size: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #ccc;
  margin: 0 0rpx 20rpx 20rpx;
}
.up_img_item{
  margin: 0 0rpx 20rpx 20rpx;
}
.form_btn_warp{
  margin-top: 20rpx;
  display: flex;
  justify-content: flex-end;
}
.form_btn_warp button{
  width: 20%;
  background-color: var(--themeColor);
  color: #fff;
  font-size: 32rpx;
  padding: 0;
  margin: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}